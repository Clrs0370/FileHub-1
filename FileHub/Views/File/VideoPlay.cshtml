@model FileHub.Models.MyFile
@{
    ViewBag.Title = Model.FileName;
    Layout = "";
}
<style type="text/css">
    body {
        width: 100%;
        height: 100%;
        margin: 0px;
        padding: 0px;
    }

    video {
        width: 100%;
        height: 100%;
        margin: 0px;
        padding: 0px;
    }
</style>
<video id="videoElement" controls="controls" autoplay="autoplay" width="100%" height="100%">
    <source src="@Model.FileUrl" type="video/mp4">
</video>


